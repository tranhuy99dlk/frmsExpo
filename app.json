      "bundleIdentifier": "com.frmsmobile",
      "config": {
        "googleMapsApiKey": "AIzaSyCgZbnDHFJ58t7u7YJmGTCHqbVsuyUyTZQ"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#FFFFFF"
      },

      "package": "com.frmsmobile",
      "config" :{
        "googleMaps":{
          "apiKey":"KEY"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
